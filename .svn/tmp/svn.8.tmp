<%-- <%@ include file = "sidebar.jsp" %> --%>
	<div class="w3-container" style="margin-left: 160px">
		<div class="jumbotron text-center">
			<h3>PJI Hotel - Customer</h3>
		</div>
		<hr>
		<div class="table-responsive">
			<table class="table table-striped table-bordered text-left">
				<thead>
					<tr>
						<th>Id Customer</th>
						<th>No. Identitas</th>
						<th>Nama Tamu</th>
						<th>No. Telp</th>
						<th>Email</th>
						<th>Alamat</th>
						<th colspan="2">Aksi</th>

					</tr>

				</thead>
				<tbody id="tbody">
					<c:forEach var="customer" items="${customerlist}">
						<tr>
							<td>${customer.idCustomer}</td>
							<td>${customer.noIdentitas}</td>
							<td>${customer.namaCustomer}</td>
							<td>${customer.noTelp}</td>
							<td>${customer.email}</td>
							<td>${customer.alamat}</td>
							<td><a href="edit-customer/${customer.idCustomer}" type="submit"
								class="btn btn-primary"><i class="fas fa-edit"></i> Edit</a></td>
							<td><a href="delete-customer/${customer.idCustomer}" class="btn btn-danger"><i class="far fa-trash-alt"></i>Delete</a></td>
						</tr>
					</c:forEach>

				</tbody>
			</table>
		</div>
		<a href="add-customers" type="submit" class="btn btn-primary"><i
			class="fas fa-user-plus"></i> Tambah Customer</a>
	</div>

	<%-- <div id="divtest" border="2px solid black">

	</div> --%>

	<script>
		function myAccFunc1() {
			var x = document.getElementById("accCheckin");
			if (x.className.indexOf("w3-show") == -1) {
				x.className += " w3-show";
				x.previousElementSibling.className += " w3-green";
			} else {
				x.className = x.className.replace(" w3-show", "");
				x.previousElementSibling.className = x.previousElementSibling.className
						.replace(" w3-green", "");
			}
		}
		function myAccFunc2() {
			var x = document.getElementById("accKamar");
			if (x.className.indexOf("w3-show") == -1) {
				x.className += " w3-show";
				x.previousElementSibling.className += " w3-green";
			} else {
				x.className = x.className.replace(" w3-show", "");
				x.previousElementSibling.className = x.previousElementSibling.className
						.replace(" w3-green", "");
			}
		}
		function myDropFunc() {
			var x = document.getElementById("demoDrop");
			if (x.className.indexOf("w3-show") == -1) {
				x.className += " w3-show";
				x.previousElementSibling.className += " w3-green";
			} else {
				x.className = x.className.replace(" w3-show", "");
				x.previousElementSibling.className = x.previousElementSibling.className
						.replace(" w3-green", "");
			}
		}
	</script>
