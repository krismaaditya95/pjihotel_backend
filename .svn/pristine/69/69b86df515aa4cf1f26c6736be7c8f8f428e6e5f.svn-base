<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta http-equiv="Pragma" content="no-cache">
<meta http-equiv="Cache-Control" content="no-cache">
<meta http-equiv="Expires" content="Sat, 01 Dec 2001 00:00:00 GMT">

    <title>Check-In / Booking</title>
    <link href="/static/css/bootstrap.min.css" rel="stylesheet">
<link href="/static/css/style.css" rel="stylesheet">

  </head>
  <body>
  <div role="navigation">
		<div class="navbar navbar-light" style="background-color: #e3f2fd;">
			<a href="/" class="navbar-brand">Dashboard</a>
			<div class="navbar-collapse collapse">
				<ul class="nav navbar-nav">
					<li><a href="all-tasks">Check in</a></li>
					<li><a href="new-task">Check out</a></li>
					<li><a href="all-tasks">Reserve a room</a></li>
					<li><a href="new-task">Add type room</a></li>
					<li><a href="all-tasks">Add room</a></li>
					<li><a href="new-task">Available room</a></li>
				</ul>
			</div>
		</div>
	</div>
	<div class="container" id="homeDiv">
		<div class="jumbotron text-center">
			<h3>Check-In / Booking</h3>
		</div>
	</div>
    <div class="container text-center" id="tasksDiv">
		<h3>Data Customer</h3>
		<hr>
    <form class="transaksiform" method="post">
      <%-- Tamu : <select class="tamuselect" name="id_customer"> --%>
      <%-- <c:forEach var="customer" items="${customerdata}">
        <option value="${customerdata.id_customer}">${customerdata.nama_customer}</option>
      </c:forEach> --%>
      <%-- </select> --%>


      <c:forEach var="cust" items="${customerdata}">
        <tr>
          <td>${cust.idCustomer}</td>
          <td>${cust.namaCustomer}</td>
        </tr>
      </c:forEach>

      Tamu : <input type="number" name="id_customer" placeholder="id_customer">
      <hr>
      Kamar : <input type="number" name="id_room" placeholder="id_room">
      <hr>
      Tanggal Check In : <input type="date" name="check_in_date" placeholder="check_in_date">
      <hr>
      Waktu Check in : <input type="time" name="check_in_time" placeholder="check_in_time">
      <hr>
      Tanggal Check Out : <input type="date" name="check_out_date" placeholder="check_out_date">
      <hr>
      Waktu Check Out : <input type="time" name="check_out_time" placeholder="check_in_date">
      <hr>
      Status Check : <input type="number" name="id_status_check" placeholder="status_check">
      <hr>
      Total Harga : <input type="number" name="total_harga" placeholder="Total Harga">

      <input type="submit" value="TAMBAH"></input>
    </form>
    <script src="/static/js/jquery-1.11.1.min.js"></script>
    <script src="/static/js/bootstrap.min.js"></script>

  </body>
</html>
